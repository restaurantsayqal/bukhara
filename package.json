{
  "name": "restaurant",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^12.6.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "optimize:images": "node scripts/optimize-images.js",
    "fix:gallery": "node scripts/fix-gallery.js",
    "optimize:all": "npm run optimize:images && npm run fix:gallery",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:prod": "npm run optimize:all && react-scripts build",
    "predeploy": "npm run build:prod && node scripts/prepare-deploy.js",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.3.0",
    "glob": "^10.3.10",
    "postcss": "^8.4.24",
    "sharp": "^0.33.3",
    "sharp-cli": "^5.1.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.2"
  },
  "homepage": "https://restaurantsayqal.github.io/bukhara"
}
